<h1>Contacts</h1>

<table class="table table-bordered table-striped">
    <thead>
      <tr>
          <th>ID</th>
          <th>E-mail</th>
          <th>Message</th>
          <th>Actions</th>
      </tr>
    </thead>
    <tbody>
    {{#each model as |message|}}
    	<tr>
        <th>{{message.id}}</th>
	    	<th>{{message.email}}</th>
	    	<td>{{message.message}}</td>
        <td><button class="btn btn-danger btn-xs" {{action 'deleteLibrary' message}}>Delete</button></td>
    	</tr>
    {{/each}}
    </tbody>
</table>